CREATE TABLE PictogramEntity(
    id INTEGER NOT NULL,
    name TEXT NOT NULL,
    PRIMARY KEY (id)
);

getById:
    SELECT * FROM PictogramEntity WHERE id = :id;

getByIds:
    SELECT * FROM PictogramEntity WHERE id IN :id;

insert:
    INSERT INTO PictogramEntity(id, name)
    VALUES (:id, :name);

insertEntity:
    INSERT INTO PictogramEntity(id, name)
    VALUES ?;

deleteAll:
    DELETE FROM PictogramEntity;
