CREATE TABLE MenuEntity(
    id INTEGER NOT NULL,
    subsystems TEXT NOT NULL,
    PRIMARY KEY (id)
);

getMenus:
    SELECT * FROM MenuEntity;

insert:
    INSERT INTO MenuEntity(id, subsystems)
    VALUES(:id, :subsystems);

insertEntity:
    INSERT INTO MenuEntity(id, subsystems)
    VALUES ?;

deleteAll:
    DELETE FROM MenuEntity;
