CREATE TABLE SubsystemEntity(
    id INTEGER NOT NULL,
    name TEXT NOT NULL,
    opened INTEGER AS Boolean NOT NULL,
    isImportant INTEGER AS Boolean NOT NULL,
    PRIMARY KEY (id)
);

getAll:
    SELECT * FROM SubsystemEntity;

insert:
    INSERT INTO SubsystemEntity(id, name, opened, isImportant)
    VALUES (:id, :name, :opened, :isImportant);

insertEntity:
    INSERT INTO SubsystemEntity(id, name, opened, isImportant)
    VALUES ?;

deleteAll:
    DELETE FROM SubsystemEntity;
